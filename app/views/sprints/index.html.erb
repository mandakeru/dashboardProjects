<div class="box box-primary">
  <div class="box-body">
    <p id="notice"><%= notice %></p>

<h1>Todos os Sprints</h1>

<table class="table table-bordered table-striped dataTable">
  <thead>
    <tr>
      <th>In√≠cio</th>
      <th>Final</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sprints.each do |sprint| %>
      <tr>
        <td><%= sprint.initial_date %></td>
        <td><%= sprint.final_date %></td>
        <td><%= link_to 'Ver', sprint %></td>
        <td><%= link_to 'Editar', edit_sprint_path(sprint) %></td>
        <td><%= link_to 'Deletar', sprint, method: :delete, data: { confirm: 'Confirma?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<button class="btn btn-default"><%= link_to 'Criar Sprint', new_sprint_path %></button>

    
  </div>
</div>
  


